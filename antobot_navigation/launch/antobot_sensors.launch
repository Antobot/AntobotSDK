<launch>

    <!-- RTK-GNSS launch -->
    <node if="true" pkg="antobot_gps" type="antobot_gps_node.py" name="rtk_gnss" />

    <!-- BOSCH IMU launch -->
    <include if="true" file="$(find imu_bno055)/launch/imu.launch" />

    <!-- Leishen 3d lidar launch (front:C32 back:C16) -->
    <include if="true" file="$(find antobot_navigation)/launch/lslidar_double_c32_c16.launch" />

</launch>
